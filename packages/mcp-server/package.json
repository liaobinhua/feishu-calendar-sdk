{
  "name": "@liaobinhua/feishu-calendar-mcp",
  "version": "0.1.0",
  "type": "module",
  "description": "MCP Server for Feishu/Lark Calendar API",
  "bin": {
    "feishu-calendar-mcp": "./dist/index.js"
  },
  "exports": {
    ".": "./src/index.ts"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target node",
    "dev": "bun run --watch src/index.ts",
    "test": "vitest",
    "start": "node dist/index.js"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "feishu",
    "lark",
    "calendar",
    "飞书",
    "日历"
  ],
  "dependencies": {
    "@liaobinhua/feishu-calendar-sdk": "workspace:*",
    "zod": "^4.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "bun-types": "latest"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "liaobinhua <liaobinhua1994@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/liaobinhua/feishu-calendar-sdk.git"
  }
}
