{
  "name": "@liaobinhua/feishu-calendar-sdk",
  "version": "0.1.0",
  "type": "module",
  "description": "Feishu/Lark Calendar SDK | 飞书日历 TypeScript SDK",
  "main": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "files": [
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "feishu",
    "lark",
    "calendar",
    "飞书",
    "日历",
    "typescript",
    "sdk"
  ],
  "dependencies": {
    "undici": "^6.0.0",
    "lru-cache": "^10.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "liaobinhua <liaobinhua1994@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/liaobinhua/feishu-calendar-sdk.git"
  }
}
