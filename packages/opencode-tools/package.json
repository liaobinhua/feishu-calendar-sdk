{
  "name": "@liaobinhua/feishu-calendar-opencode",
  "version": "0.1.0",
  "type": "module",
  "description": "OpenCode custom tools for Feishu Calendar SDK",
  "main": "./dist/index.js",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target node",
    "build:cjs": "bun build src/index.ts --outfile dist/index.cjs --target node",
    "build:esm": "bun build src/index.ts --outfile dist/index.mjs --target node",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "opencode",
    "feishu",
    "lark",
    "calendar",
    "tools",
    "custom-tools"
  ],
  "peerDependencies": {
    "@liaobinhua/feishu-calendar-sdk": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "liaobinhua <liaobinhua1994@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/liaobinhua/feishu-calendar-sdk.git"
  }
}
